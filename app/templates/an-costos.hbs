{{outlet}}

<div class="row">
		<nav>
			<div class="nav-wrapper blue lighten-2">
			  <a href="#" class="brand-logo center"><h5>Analisis de costos</h5></a>
			</div>
		</nav>
</div>

 <div class= "container s6 offset-s3">
        
        <div class="row">
            <form class="col s6">
                <div class="row">
                    <div class="input-field col s12">
                        <i class="material-icons prefix">monetization_on</i>
                        {{input id="icon_prefix" type="text" class="validate" value=toSearch}}
                        <label for="icon_prefix">Presupuesto de inversion</label>
                    </div>
                    <a {{action 'search' toSearch}} class="waves-effect waves-light btn">Buscar</a>
                </div>
            </form>
        </div>

         <div class="row">
	                <table class="striped col s12">
				        <thead>
				          <tr>
				              <th>Prioridad</th>
				              <th>Proyecto</th>
				              <th>Costo</th>
				          </tr>
				        </thead>
				        <tbody>
				        	
				        	{{#each model as |proyecto|}}
				        	<tr>
				        		<td>{{proyecto.prioridad}}</td>
				        		<td>{{proyecto.nombre}}</td>
				        		<td>{{proyecto.costo}}</td>
				        	</tr>
				        	{{/each}}	
				        </tbody>
	      			</table>
	     </div>
	     <div class="row">
	      			<ul class="collection with-header col s12">

				        <li class="collection-header"><h5>Proyectos prioritarios ejecutables con el presupuesto de inversi√≥n</h5></li>
				        {{#each todoProjects as |proyecto|}}
				       		<li class="collection-item">{{proyecto.nombre}}</li>

	      				
	      				{{/each}}
				     
     				</ul>
        </div>
    </div>

    <footer class="page-footer blue lighten-1">
		<div class="row">
			<div class="col s2 offset-s1">
				{{#link-to 'matDecision' tagName="button" class= "btn waves-effect waves-light blue lighten-1"}}<i class="material-icons right">arrow_back</i>{{/link-to}}
			</div>
		</div>
	</footer>


