
	<div class="row">
		<nav>
			<div class="nav-wrapper blue lighten-2">
			  <a href="#" class="brand-logo center"><h5>Matríz de decisión con ponderación y peso</h5></a>
			</div>
		</nav>
	</div>

    <div class="container s8 offset-s2">
        <div class="row center">
            <h5>Interpretación de los pesos obtenidos: MAYOR es MEJOR</h5>
        </div>
        <div class="row">
            <div class="col s10 offset-s1 center">
                <h5>1 = Muy baja</h5><i class="material-icons center">thumb_down</i> <h5>y</h5> <h5>{{np}} = Sobresaliente</h5><i class="material-icons center">thumb_up</i>
            </div>
        </div>
        <div class="row">
            <div class="col s8 offset-s2">
                <table>
                    <thead>
                        <tr>
                            <th style="width:200px;">Criterio</th>
                            <th style="width:200px">Ponderación</th>
                            {{#each model.proyectos as |proyecto|}}
                                <th style="width:200px">{{proyecto.nombre}} </th>
                            {{/each}}
                        </tr>
                    </thead>
                    <tbody>
                        {{#each model.criterios as |criterio|}}
                                {{calif-mat criterio=criterio proyectos=model.proyectos tagName="tr"}}

                        {{/each}}
                        <tr>
                            <td></td>
                            <td>Total</td>
                            {{#each model.proyectos as |proyecto|}}
                                <td>{{proyecto.total}}</td>
                            {{/each}}
                        </tr>
                        <tr>
                            <td></td>
                            <td>Prioridad</td>
                            {{#each model.proyectos as |proyecto|}}
                                <td>{{proyecto.prioridad}}</td>
                            {{/each}}
                        </tr>

                    </tbody>
                </table>
            </div>
        </div>
    </div>

    {{#ember-wormhole to="footer"}}
    <div class="col l2 offset-l1">

        {{#link-to 'asigValores' tagName="button" class= "btn waves-effect waves-light grey darken-1"}}<i class="material-icons right">arrow_back</i>{{/link-to}}
    </div>
    <div class="col l2 offset-l7">
        {{#link-to 'anCostos' tagName="button" class= "btn waves-effect waves-light grey darken-1"}}<i class="material-icons right">arrow_forward</i>{{/link-to}}
    </div>
    {{/ember-wormhole}}
